<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projcrech">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".LoadAppActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MailBoxActivity" />
        <activity android:name=".EventEditActivity" />
        <activity android:name=".WeekViewActivity"/>
        <activity android:name=".DailyCalendarActivity" />
        <activity android:name=".GridItemActivity" />
        <activity android:name=".LunchActivity" />
        <activity android:name=".HealthActivity" />
        <activity android:name=".SnackActivity" />
        <activity android:name=".DiaperActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".ChildList" />
    </application>

</manifest>